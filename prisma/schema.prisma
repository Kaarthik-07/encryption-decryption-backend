// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model Hash{
  id        Int      @id @default(autoincrement())
  original  String
  hashed    String
}
model Caeser{
   id        Int      @id @default(autoincrement())
   plaintext String
   hashed String
   operation String
   key Int

}
model Base64{
   id        Int      @id @default(autoincrement())
   plaintext String
   hashed String
   operation String
   

}